@using Com.Cos.Models
@using Com.Cos.Web.Areas.Member.Models

@{
    ViewBag.Title = "我的参与";
}
@section Css{
    <link rel="stylesheet" href="~/Content/css/libs/ReleaseCps.css">
}
<div class="big">
    <div class="content clearfix">
        <!--左侧菜单 Start-->
        @Html.Partial("_PartialLeftMenu")
        <!--左侧菜单 End-->

        <div class="middleContent" id="tb">
            @Html.Partial("_PartialParticipateExchange", new PublishViewModel { Model = (List<Com.Cos.Models.Exchange>)ViewBag.Exchanges, PageIndex = (int)ViewBag.PageIndex, PageSize = (int)ViewBag.PageSize, TotalRecord = (int)ViewBag.TotalRecord })
        </div>
        <!--右侧 Start-->
        @*@Html.Partial("_PartialRight")*@
        <!--右侧 End-->
    </div>
</div>

@section Scripts
{
    @Scripts.Render("~/bundles/jqueryval")
}
